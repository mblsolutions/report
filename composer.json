{
  "name": "mblsolutions/report",
  "description": "Laravel Report package",
  "keywords": ["redu", "report", "laravel"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "MBL Solutions Ltd.",
      "email": "support@mblsolutions.co.uk"
    }
  ],
  "require": {
    "php": "^7.4||^8.0",
    "ext-json": "*",
    "illuminate/cache": "^8.0||^9.0",
    "illuminate/console": "^8.0||^9.0",
    "illuminate/contracts": "^8.0||^9.0",
    "illuminate/database": "^8.0||^9.0",
    "illuminate/events": "^8.0||^9.0",
    "illuminate/filesystem": "^8.0||^9.0",
    "illuminate/queue": "^8.0||^9.0",
    "illuminate/support": "^8.0||^9.0",
    "illuminate/http": "^8.0||^9.0",
    "maatwebsite/excel": "^3.1",
    "lerouse/laravel-repository": "^1.0||^2.0",
    "laravel/legacy-factories": "^1.2",
    "doctrine/dbal": "^3.3"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^4.0||^5.0||^6.0||^7.0",
    "phpunit/phpunit": "^7.3||^8.2||^9.3"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\Report\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\Report\\ReportServiceProvider"
      ]
    }
  }
}
