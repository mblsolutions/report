{
  "name": "mblsolutions/report",
  "description": "Laravel Report package",
  "keywords": ["redu", "report", "laravel"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "MBL Solutions Ltd.",
      "email": "support@mblsolutions.co.uk"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "illuminate/cache": "^9.0|^10.0",
    "illuminate/console": "^9.0|^10.0",
    "illuminate/contracts": "^9.0|^10.0",
    "illuminate/database": "^9.0|^10.0",
    "illuminate/events": "^9.0|^10.0",
    "illuminate/filesystem": "^9.0|^10.0",
    "illuminate/queue": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "illuminate/http": "^9.0|^10.0",
    "maatwebsite/excel": "^3.1",
    "lerouse/laravel-repository": "^2.4",
    "laravel/legacy-factories": "^1.2",
    "doctrine/dbal": "^3.3",
    "psr/simple-cache": "^2.0"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\Report\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\Report\\ReportServiceProvider"
      ]
    }
  }
}
