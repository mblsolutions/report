
{
  "name": "mblsolutions/report",
  "description": "MBL Solutions Laravel Reports",
  "keywords": ["mblsolutions", "report", "laravel"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "MBL Solutions Ltd.",
      "email": "support@mblsolutions.co.uk"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "illuminate/console": "~5.6.0|~5.7.0|~5.8.0|^6.0",
    "illuminate/events": "~5.6.0|~5.7.0|~5.8.0|^6.0",
    "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0",
    "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0",
    "illuminate/http": "~5.6.0|~5.7.0|~5.8.0|^6.0"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\Report\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\Report\\ReportServiceProvider"
      ]
    }
  }
}